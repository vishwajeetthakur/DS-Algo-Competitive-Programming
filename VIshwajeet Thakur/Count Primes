class Solution:
    def countPrimes(self, n: int) -> int:
        
        if n<=2:
            return 0
        
        prime=[1]*n
        
        for i in range(2,n):
            if prime[i]:
                for j in range(i*i,n,i):
                    prime[j]=0
        return sum(prime)-2
                
        
