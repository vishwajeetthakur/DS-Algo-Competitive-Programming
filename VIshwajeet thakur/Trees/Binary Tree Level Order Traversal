# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, val=0, left=None, right=None):
#         self.val = val
#         self.left = left
#         self.right = right
class Solution:
    def levelOrder(self, root: TreeNode) -> List[List[int]]:
        if root==None:
            return root
        queue = [root]
        ans = []
        while queue:
            updated_queue = []
            temp = []
            for i in queue:
                temp.append(i.val)
                if i.left:
                    updated_queue.append(i.left)
                if i.right:
                    updated_queue.append(i.right)
            ans.append(temp)
            queue = updated_queue
        return ans
            
