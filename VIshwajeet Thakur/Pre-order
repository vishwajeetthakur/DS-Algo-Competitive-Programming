# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, val=0, left=None, right=None):
#         self.val = val
#         self.left = left
#         self.right = right
class Solution:
    def preorderTraversal(self, root: TreeNode) -> List[int]:
        def visit(node, li):
            if node==None:
                return
            
            li.append(node.val)
            visit(node.left, li)
            visit(node.right, li)

        visited = []
        visit(root, visited)
        return visited
            
        
